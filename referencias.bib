@article{Costa2012,
abstract = {Com o crescente uso de Ambientes Virtuais de Aprendizagem (AVA) na Web e outras tecnologiaspara apoio ao processo de ensino e aprendizagem, um grande volume de dadostem sido gerado a partir das diferentes modalidades de intera{\c{c}}{\~{a}}o no sistema envolvendoprincipalmente estudantes e professores. Entretanto, boa parte desses dados n{\~{a}}o t{\^{e}}m sido analisados, o que se constitui numa lacuna importante, dada a quantidade de informa{\c{c}}{\~{a}}ovaliosa que se pode potencialmente extrair de tais dados. Por exemplo, em buscade melhor compreender o comportamento dos estudantes e a forma como eles aprendem,o trabalho realizado por pesquisadores em Minera{\c{c}}{\~{a}}o de Dados Educacionais tem investidono uso e na melhoria de conhecidas t{\'{e}}cnicas de minera{\c{c}}{\~{a}}o de dados para obterconhecimentos relevantes a partir desses dados. Tais conhecimentos podem servir de subs{\'{i}}diopara a melhoria das pr{\'{a}}ticas em educa{\c{c}}{\~{a}}o a dist{\^{a}}ncia ou presencial, al{\'{e}}m de seruma importante ferramenta para viabilizar a personaliza{\c{c}}{\~{a}}o do ensino. O objetivo destecap{\'{i}}tulo {\'{e}} introduzir os principais conceitos, t{\'{e}}cnicas e algoritmos de minera{\c{c}}{\~{a}}o de dadoscom aplica{\c{c}}{\~{o}}es a conjuntos de dados educacionais. Neste sentido, ser{\~{a}}o discutidosm{\'{e}}todos sobre as seguintes tarefas: Classifica{\c{c}}{\~{a}}o e regress{\~{a}}o, agrupamento de dados,minera{\c{c}}{\~{a}}o de regras de associa{\c{c}}{\~{a}}o. Particularmente, ser{\~{a}}o discutidos m{\'{e}}todos especialmentedesenvolvidos para predi{\c{c}}{\~{a}}o dos conhecimentos, emo{\c{c}}{\~{o}}es, e aprendizagem futurado estudante.},
author = {Costa, Evandro and Baker, Ryan S J and Amorim, Lucas and Magalh{\~{a}}es, Jonathas},
file = {:home/alexandre/Downloads/2341-3764-1-PB.pdf:pdf},
mendeley-groups = {Mestrado/Artigos - MDE},
pages = {1--29},
title = {{Minera{\c{c}}{\~{a}}o de Dados Educacionais: Conceitos, T{\'{e}}c- nicas, Ferramentas e Aplica{\c{c}}{\~{o}}es}},
volume = {d},
year = {2012}
}

